{
  "entryPoints": [
    "src/api/index.ts",
    "src/types/index.ts",
    "src/services/index.ts"
  ],
  "plugin": ["typedoc-plugin-markdown"],
  "out": "docs/auto-docs",
  "cleanOutputDir": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "hideBreadcrumbs": true,
  "hidePageHeader": true,
  "readme": "none",
  "entryFileName": "index.md",
  "outputFileStrategy": "members"
}

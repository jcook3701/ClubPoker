{
  "name": "ClubPoker",
  "version": "1.0.1",
  "main": "index.js",
  "license": "AGPL-3.0-or-later",
  "author": "Jared Cook",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "webpack --config webpack.config.js",
    "zip": "npm run build && cd dist && zip -r ../ClubPoker.zip .",
    "watch": "webpack -w --config webpack.config.js",
    "storybook": "sb dev",
    "build-storybook": "sb build",
    "lint-fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'&& stylelint 'src/**/*.{css,scss}'",
    "lint-check": "eslint 'src/**/*.{js,jsx,ts,tsx,json}'&& stylelint 'src/**/*.{css,scss}'",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "pretty-check": "prettier --check \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "docs": "typedoc && node scripts/inject-front-matter.js",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@ionic/react": "^8.4.1",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.3.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "luxon": "^3.7.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-select": "^5.9.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@storybook/addon-actions": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-links": "^6.3.0",
    "@storybook/addon-webpack5-compiler-swc": "^2.0.0",
    "@storybook/cli": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-webpack5": "^8.4.7",
    "@types/chrome": "^0.0.292",
    "@types/node": "^24.7.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-select": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-typescript": "^0.14.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^8.0.0",
    "lint-staged": "^16.2.7",
    "mini-css-extract-plugin": "^2.9.4",
    "prettier": "^2.3.2",
    "react-inspector": "^6.0.2",
    "sass": "^1.83.1",
    "sass-loader": "^16.0.4",
    "standard-version": "^9.5.0",
    "style-loader": "^4.0.0",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^36.0.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.13",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ]
  }
}

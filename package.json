{
  "name": "ClubPoker",
  "version": "1.0.1",
  "main": "index.js",
  "license": "AGPL-3.0-or-later",
  "author": "Jared Cook",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "storybook": "storybook dev",
    "build-storybook": "storybook build",
    "lint-fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\" && stylelint --fix \"src/**/*.{css,scss}\"",
    "lint-check": "eslint \"src/**/*.{js,jsx,ts,tsx}\" && stylelint \"src/**/*.{css,scss}\"",
    "pretty": "prettier --write \"**/*.{js,jsx,mjs,cjs,ts,tsx,json,css,scss}\"",
    "pretty-check": "prettier --check \"**/*.{js,jsx,mjs,cjs,ts,tsx,json,css,scss}\"",
    "spellcheck": "cspell 'src/**/*.ts'",
    "docs": "typedoc && node scripts/inject-front-matter.js",
    "prepare": "husky",
    "release": "standard-version"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@ionic/react": "^8.4.1",
    "@mui/icons-material": "^6.4.1",
    "@mui/material": "^6.4.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "luxon": "^3.7.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-select": "^5.10.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.26.0",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@storybook/addon-essentials": "9.0.0-alpha.12",
    "@storybook/addon-links": "10.3.0-alpha.4",
    "@storybook/addon-vitest": "10.3.0-alpha.4",
    "@storybook/react": "10.3.0-alpha.4",
    "@storybook/react-vite": "10.3.0-alpha.4",
    "@storybook/test": "9.0.0-alpha.2",
    "@types/chrome": "^0.0.299",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-select": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@wxt-dev/module-react": "^1.1.5",
    "cspell": "^9.6.4",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-storybook": "0.11.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "mini-css-extract-plugin": "^2.9.4",
    "prettier": "^3.5.0",
    "sass-embedded": "^1.97.3",
    "sass-loader": "^16.0.4",
    "standard-version": "^9.5.0",
    "storybook": "10.3.0-alpha.4",
    "style-loader": "^4.0.0",
    "stylelint": "^16.14.1",
    "stylelint-config-standard": "^37.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-replace-paths": "^0.0.14",
    "typedoc": "^0.28.13",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3",
    "vite": "^6.1.1",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "3.0.5",
    "wxt": "^0.20.6"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ]
  }
}

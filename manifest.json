{
  "manifest_version": 3,
  "name": "Tournament to Google Calendar",
  "description": "This is a Chrome extension intended to interface with clubwpt.com",
  "version": "1.0",
  "permissions": ["identity", "activeTab", "scripting"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": "icons/icon48.png",
    "default_popup": "js/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://clubwpt.com/*", "https://*.clubwpt.com/*"],
      "js": ["js/content.js"]
    }
  ]
}
